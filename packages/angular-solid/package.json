{
  "name": "@fredbnm/angular-solid-icons",
  "private": false,
  "version": "1.0.6",
  "license": "MIT",
  "description": "Angular Solid Icons Set",
  "authors": [
    "Tiago Gerken <gerken@getpingback.com>",
    "Pedro Ladeira <pedro@getpingback.com>"
  ],
  "ngPackage": {
    "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
    "dest": "./dist",
    "lib": {
      "entryFile": "src/public-api.ts"
    }
  },
  "main": "dist/angular-solid-icons.umd.js",
  "module": "dist/angular-solid-icons.esm.js",
  "es2015": "dist/angular-solid-icons.esm.js",
  "esm5": "dist/angular-solid-icons.esm.js",
  "fesm5": "dist/angular-solid-icons.esm.js",
  "fesm2015": "dist/angular-solid-icons.esm.js",
  "typings": "dist/angular-solid-icons.d.ts",
  "metadata": "dist/angular-solid-icons.metadata.json",
  "sideEffects": false,
  "files": [
    "dist/",
    "README.md",
    "src/"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fredbnm/icons.git"
  },
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "watch": "ng-packagr -p ng-package.json --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@angular/core": "^12.0.0 || ^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0",
    "@angular/common": "^12.0.0 || ^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0",
    "rxjs": "^6.5.0 || ^7.0.0 || ^8.0.0"
  },
  "devDependencies": {
    "@angular/animations": "^20.1.0",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/compiler-cli": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "ng-packagr": "^20.1.0",
    "rxjs": "~7.8.0",
    "typescript": "~5.8.0",
    "zone.js": "~0.15.0"
  }
}
